{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 8,
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-control-regex": "off",
        "linebreak-style": ["warn", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": 0,
        "no-unused-vars": ["warn"],
        "no-empty": 0
    }
}